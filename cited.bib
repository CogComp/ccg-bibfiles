@Article{STJTT16,
  Title                    = {{Exploration from Demonstration for Interactive Reinforcement Learning}},
  Author                   = {Subramanian, Kaushik and Tech, Georgia and Jr, Charles L Isbell and Tech, Georgia and Thomaz, Andrea L},
